<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Land Types Master Data -->
        <record id="land_type_single_crop" model="bhu.land.type">
            <field name="code">SINGLE_CROP</field>
            <field name="name">एक फसली</field>
            <field name="description">Single crop land</field>
            <field name="active">True</field>
        </record>

        <record id="land_type_double_crop" model="bhu.land.type">
            <field name="code">DOUBLE_CROP</field>
            <field name="name">दो फसली</field>
            <field name="description">Double crop land</field>
            <field name="active">True</field>
        </record>

        <record id="land_type_fallow" model="bhu.land.type">
            <field name="code">FALLOW</field>
            <field name="name">पड़ती</field>
            <field name="description">Fallow land</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
