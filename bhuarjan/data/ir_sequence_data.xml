<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Survey Numbers -->
        <record id="seq_bhu_survey" model="ir.sequence">
            <field name="name">Survey Number</field>
            <field name="code">bhu.survey</field>
            <field name="prefix">SUR_</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Sequence for Notification 4 Numbers -->
        <record id="seq_bhu_notification4" model="ir.sequence">
            <field name="name">Notification 4 Number</field>
            <field name="code">bhu.notification4</field>
            <field name="prefix">NOT4_</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
