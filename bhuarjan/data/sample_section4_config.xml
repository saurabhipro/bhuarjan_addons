        <!-- Sample Hindi Content Templates for Section 4 Notification -->
        <!-- These can be used as reference when filling wizard forms -->
        
        <!-- Sample Data Template 1 -->
        <record id="section4_sample_template_1" model="ir.config_parameter">
            <field name="key">bhuarjan.section4.sample.public_purpose</field>
            <field name="value"><![CDATA[<p><strong>रिंग रोड रायगढ़</strong> परियोजना के लिए भूमि अर्जन। यह परियोजना शहर के यातायात को सुविधाजनक बनाने और आर्थिक विकास को बढ़ावा देने के लिए है।</p>]]></field>
        </record>
        
        <record id="section4_sample_template_q1" model="ir.config_parameter">
            <field name="key">bhuarjan.section4.sample.q1</field>
            <field name="value"><![CDATA[<p><strong>लोक प्रयोजन:</strong> रिंग रोड रायगढ़ परियोजना के अंतर्गत जुर्दा ग्राम में <strong>3.0000 हेक्टेयर</strong> भूमि का अर्जन किया जा रहा है। यह परियोजना शहर के यातायात प्रबंधन में सुधार लाएगी और आर्थिक विकास को गति देगी।</p>]]></field>
        </record>
        
        <record id="section4_sample_template_q7" model="ir.config_parameter">
            <field name="key">bhuarjan.section4.sample.q7</field>
            <field name="value"><![CDATA[<p><strong>विकल्पों का अध्ययन:</strong></p><ul><li>वैकल्पिक मार्गों का सर्वेक्षण किया गया</li><li>न्यूनतम प्रभाव वाला मार्ग चुना गया</li><li>पर्यावरणीय प्रभाव का मूल्यांकन किया गया</li></ul>]]></field>
        </record>
        
        <record id="section4_sample_template_q9" model="ir.config_parameter">
            <field name="key">bhuarjan.section4.sample.q9</field>
            <field name="value"><![CDATA[<p><strong>परियोजना के लाभ:</strong></p><ul><li>यातायात की सुविधा में सुधार</li><li>आर्थिक विकास में वृद्धि</li><li>रोजगार के अवसरों में वृद्धि</li><li>शहरी विकास में योगदान</li></ul>]]></field>
        </record>
        
        <record id="section4_sample_template_q10" model="ir.config_parameter">
            <field name="key">bhuarjan.section4.sample.q10</field>
            <field name="value"><![CDATA[<p><strong>प्रतिकर उपाय:</strong></p><ul><li>भूमि मालिकों को उचित मुआवजा</li><li>पुनर्वासन पैकेज</li><li>रोजगार के अवसर</li><li>शिक्षा और स्वास्थ्य सुविधाएं</li></ul><p><strong>अनुमानित व्यय:</strong> ₹ 5 करोड़</p>]]></field>
        </record>
        
        <record id="section4_sample_template_q11" model="ir.config_parameter">
            <field name="key">bhuarjan.section4.sample.q11</field>
            <field name="value"><![CDATA[<p><strong>अन्य प्रभावित घटक:</strong></p><ul><li>कृषि भूमि</li><li>वन क्षेत्र</li><li>जल स्रोत</li><li>सड़क और बिजली की सुविधाएं</li></ul>]]></field>
        </record>
        
