<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sample Section 11 Preliminary Reports (2 records - one per village: Pandripani, Gopalpur) -->
        <!-- Only for villages that have Section 4 notifications -->
        <!-- Names will be auto-generated using sequence: NOT11_{project_code}_{village_code} -->
        <!-- Note: Jurda and Bhikharimal do not have Section 4 notifications, so no Section 11 reports -->
        <record id="section11_report_pandripani" model="bhu.section11.preliminary.report">
            <field name="name">NOT11_RRR-001_PP</field>
            <field name="project_id" ref="project_ring_road_raigarh"/>
            <field name="village_id" ref="pandripani_village"/>
            <field name="section4_notification_id" ref="section4_notification_pandripani"/>
            <field name="state">draft</field>
            <field name="notification_number">/अ-83/2024-25</field>
            <field name="publication_date">2024-12-05</field>
        </record>
        
        <record id="section11_report_gopalpur" model="bhu.section11.preliminary.report">
            <field name="name">NOT11_RRR-001_GL</field>
            <field name="project_id" ref="project_ring_road_raigarh"/>
            <field name="village_id" ref="gopalpur_village"/>
            <field name="section4_notification_id" ref="section4_notification_gopalpur"/>
            <field name="state">draft</field>
            <field name="notification_number">/अ-84/2024-25</field>
            <field name="publication_date">2024-12-10</field>
        </record>
    </data>
</odoo>

