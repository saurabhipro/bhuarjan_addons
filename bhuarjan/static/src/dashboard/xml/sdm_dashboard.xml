                        <div class="o_stat_card o_card_sia" t-on-click="() => this.openSectionList('bhu.sia.team')">
                            <div class="o_stat_icon">
                                <i class="fa fa-users"/>
                            </div>
                            <div class="o_stat_content">
                                <div class="o_stat_label">SIA Team</div>
                                <div class="o_stat_value"><t t-esc="state.stats ? state.stats.sia_total : 0"/></div>
                                <div class="o_stat_states">
                                    <span><i class="fa fa-paper-plane"/> <t t-esc="state.stats ? state.stats.sia_submitted : 0"/></span>
                                    <span><i class="fa fa-check-circle"/> <t t-esc="state.stats ? state.stats.sia_approved : 0"/></span>
                                    <span><i class="fa fa-undo"/> <t t-esc="state.stats ? state.stats.sia_send_back : 0"/></span>
                                </div>
                            </div>
                            <button class="btn btn-sm btn-light o_create_btn" t-on-click.stop="() => this.createSectionRecord('bhu.sia.team')" title="Create New">
                                <i class="fa fa-plus"/>
                            </button>
